<?xml version="1.0" ?>
<launch>

    <!-- IMU -->
    <node
        pkg="tf2_ros"
        type="static_transform_publisher"
        name="base_link_to_imu0"
        args="0 0 0 0 0 0 base_link imu_0"
    />

    <!-- DVL -->
    <node pkg="tf2_ros"
          type="static_transform_publisher"
          name="base_link_to_UNS"
          args="0 0 0 0 0 0 base_link uns_link"
    />


</launch> 